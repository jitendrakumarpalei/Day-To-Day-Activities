kubectl create namespace dev
kubectl get ns
kubectl run dev-nginx-pod \
  --image=nginx:latest \
  --namespace=dev
kubectl get pods -n dev
Create a file: dev-nginx-pod.yaml
apiVersion: v1
kind: Pod
metadata:
  name: dev-nginx-pod
  namespace: dev
spec:
  containers:
    - name: nginx
      image: nginx:latest
kubectl apply -f dev-nginx-pod.yaml
kubectl get pods -n dev
